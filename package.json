{
  "name": "fbw-simbridge",
  "version": "0.5.4",
  "description": "The simbridge server for FBW addons for various tasks the addons themselves can't achieve",
  "author": "",
  "private": false,
  "license": "GPL-3.0-only",
  "os": [
    "win32"
  ],
  "engines": {
    "node": "20.x.x"
  },
  "scripts": {
    "prepub": "copy package.json dist && copy README.md dist && copy LICENSE dist && copy CODE_OF_CONDUCT.md dist",
    "pub": "cd dist && npm publish",
    "postpub": "cd dist && del package.json && del README.md && del LICENSE && del CODE_OF_CONDUCT.md",
    "prebuild": "rimraf dist && rimraf build && rimraf build-modules",
    "build": "npm run build:server && npm run build:mcdu",
    "build:mcdu": "webpack --config apps/mcdu/webpack.config.js",
    "build:server": "nest build server",
    "build:exec": "npm run build && mkdir build && npm run install:exec",
    "build:package": "node scripts/package.js",
    "install:exec": "node doExe.mjs && npm run copy:deps && npm run build:package",
    "copy:deps": "npm run copy:resources && npm run copy:modules && copyfiles -s -f LICENSE build && npm run fetch:terrain",
    "copy:resources": "copyfiles -s -f apps/server/src/config/properties.json ./node_modules/pdf-to-printer/dist/*.exe build/resources && renamer -f \"/Sumatra.*/i\" -r SumatraPDF.exe build/resources/*",
    "copy:modules": "npm run copy:pdfjs && npm run copy:nodehide && npm run copy:sharp && npm run copy:open",
    "copy:pdfjs": "copyfiles -s -f \"node_modules/pdfjs-dist/build/**/*.*\" build/node_modules/pdfjs-dist/build && npm run copy:pdfjs:fonts && npm run copy:pdfjs:cmaps",
    "copy:pdfjs:fonts": "copyfiles -s -f \"node_modules/pdfjs-dist/standard_fonts/**/*.*\" build/node_modules/pdfjs-dist/standard_fonts",
    "copy:pdfjs:cmaps": "copyfiles -s -f \"node_modules/pdfjs-dist/cmaps/**/*.*\" build/node_modules/pdfjs-dist/cmaps",
    "copy:nodehide": "copyfiles -s -f \"node_modules/node-hide-console-window/build/Release/**/*\" build/node_modules/node-hide-console-window/build/Release",
    "copy:sharp": "copyfiles -s -f \"node_modules/sharp/build/Release/**/*\" build/node_modules/sharp/build/Release",
    "copy:open": "copyfiles -s -f \"node_modules/open/xdg-open\"  build/node_modules/open",
    "fetch:terrain": "node scripts/terrain.js",
    "start": "nest start server",
    "start:dev": "nest start server --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint  --cache **/*.{js,mjs,jsx,ts,tsx}",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/fbw-local-api/test/jest-e2e.json"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "@flybywiresim/msfs-nodejs": "^0.4.0",
    "@nestjs/axios": "^3.0.3",
    "@nestjs/common": "^10.4.4",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.4",
    "@nestjs/platform-express": "^10.4.4",
    "@nestjs/platform-ws": "^10.4.4",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/websockets": "^10.4.4",
    "@types/pdfjs-dist": "^2.10.378",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "copyfiles": "^2.4.1",
    "dns-packet": "^5.6.1",
    "gpu.js": "^2.16.0",
    "ip": "^2.0.1",
    "mime-types": "^2.1.35",
    "multicast-dns": "^7.2.5",
    "nest-winston": "^1.9.7",
    "node-hide-console-window": "^2.2.0",
    "open": "^10.1.0",
    "pdf-to-printer": "5.6.0",
    "pdfjs-dist": "^2.13.216",
    "pdfkit": "^0.15.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-use-websocket": "^4.8.1",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.5",
    "skia-canvas": "^1.0.2",
    "swagger-ui-express": "^5.0.1",
    "systray2": "^2.1.4",
    "winston": "^3.14.2",
    "winston-daily-rotate-file": "^5.0.0",
    "ws": "^8.18.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@angablue/exe": "^3.2.3",
    "@babel/core": "^7.25.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-constant-elements": "^7.25.1",
    "@babel/plugin-transform-react-inline-elements": "^7.24.7",
    "@babel/plugin-transform-runtime": "^7.25.4",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@babel/runtime": "^7.25.6",
    "@flybywiresim/fragmenter": "^0.8.0",
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.1.4",
    "@nestjs/testing": "^10.4.4",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-image": "~3.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-replace": "^6.0.1",
    "@rollup/plugin-typescript": "~12.1.0",
    "@types/dns-packet": "^5.6.5",
    "@types/express": "^5.0.0",
    "@types/ip": "^1.1.3",
    "@types/jest": "29.5.13",
    "@types/mime-types": "^2.1.4",
    "@types/multicast-dns": "^7.2.4",
    "@types/node": "^22.7.4",
    "@types/pdfkit": "^0.13.5",
    "@types/supertest": "^6.0.2",
    "@types/ws": "^8.5.12",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "axios": "^1.7.7",
    "babel-loader": "^9.2.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "esbuild": "0.24.0",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.1",
    "node-fetch": "^3.3.2",
    "pkg": "^5.8.1",
    "renamer": "^5.0.2",
    "rollup": "^4.22.5",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-css-only": "^4.5.2",
    "rollup-plugin-livereload": "~2.0.5",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-serve": "~1.1.1",
    "rollup-plugin-typescript-paths": "^1.5.0",
    "run-script-webpack-plugin": "^0.2.0",
    "source-map-support": "^0.5.21",
    "style-loader": "^4.0.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/"
    ]
  },
  "commonizer": [
    "node_modules/open"
  ],
  "pkg": {
    "targets": [
      "node20-win-x64"
    ],
    "assets": [
      "node_modules/linebreak/src/classes.trie",
      "node_modules/sharp/**/*.*",
      "node_modules/skia-canvas/**/*.*",
      "node_modules/canvas/**/*.*",
      "node_modules/pdfkit/js/data/Helvetica.afm",
      "node_modules/swagger-ui-dist/**/*.*",
      "node_modules/gpu.js/**/*.*",
      "node_modules/gl/**/*.*",
      "node_modules/gl-wiretap/**/*.*",
      "node_modules/glsl-tokenizer/**/*.*",
      "node_modules/gpu-mock.js/**/*.*",
      "node_modules/bindings/**/*.*",
      "node_modules/file-uri-to-path/**/*.*",
      "node_modules/bit-twiddle/**/*.*",
      "node_modules/@flybywiresim/msfs-nodejs/**/*.*",
      "node_modules/es-get-iterator/**/*.*",
      "/node_modules/systray2/traybin/*.exe",
      "dist/mcdu/**/*",
      "dist/assets/**/*",
      "dist/terrain/mapdata/**/*.*",
      "dist/terrain/processing/**/*.*",
      "dist/terrain/communication/**/*.*",
      "dist/terrain/fileformat/**/*.*"
    ],
    "outputPath": "build"
  }
}
